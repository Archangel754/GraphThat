{
    "name": "GraphThat",
    "description": "Quickly graph data by selecting text on a webpage.",
    "version": "0.1.0.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/GraphThat_icon16.png",
            "32": "/images/GraphThat_icon32.png",
            "48": "/images/GraphThat_icon48.png",
            "128": "/images/GraphThat_icon128.png"
        }
    },
    "icons": {
        "16": "/images/GraphThat_icon16.png",
        "32": "/images/GraphThat_icon32.png",
        "48": "/images/GraphThat_icon48.png",
        "128": "/images/GraphThat_icon128.png"
    }
}